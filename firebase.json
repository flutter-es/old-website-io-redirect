{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      { "source": "/", "destination": "https://flutter.dev/", "type":301 },
      { "source": "/:first*", "destination": "https://flutter.dev/:first", "type":301 },
      { "source": "/:first/:second", "destination": "https://flutter.dev/:first/:second", "type":301 },
    ]
  }
}
